<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="220.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ramdolpix.jwakeonlangui.Controller">
   <children>
      <Button layoutX="89.0" layoutY="151.0" mnemonicParsing="false" onAction="#wakeUpAction" text="Wake UP!" />
      <Label layoutX="14.0" layoutY="52.0" text="IP" />
      <Label layoutX="14.0" layoutY="102.0" text="MAC" />
      <TextField fx:id="ipField" layoutX="55.0" layoutY="47.0" promptText="Use Broadcast Address" text="192.168.0.255" />
      <TextField fx:id="macField" layoutX="55.0" layoutY="97.0" promptText="ex:XX:XX:XX:XX:XX:XX" />
   </children>
</AnchorPane>
